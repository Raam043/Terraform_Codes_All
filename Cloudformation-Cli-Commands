          ParameterKey=CloudWatchLogRetention,UsePreviousValue=true \
          ParameterKey=CloudWatchLowEval,UsePreviousValue=true \
          ParameterKey=CloudWatchLowOperator,UsePreviousValue=true \
          ParameterKey=CloudWatchLowPeriod,UsePreviousValue=true \
          ParameterKey=CloudWatchLowThreshold,UsePreviousValue=true \
          ParameterKey=CloudWatchScalingMetric,UsePreviousValue=true \
          ParameterKey=DesiredCapacity,UsePreviousValue=true \
          ParameterKey=DetailedMonitoring,UsePreviousValue=true \
          ParameterKey=EBSVolumeSize,UsePreviousValue=true \
          ParameterKey=EBSVolumeType,UsePreviousValue=true \
          ParameterKey=EC2ScaleDownAdjustment,UsePreviousValue=true \
          ParameterKey=EC2ScaleDownCooldown,UsePreviousValue=true \
          ParameterKey=EC2ScaleUpAdjustment,UsePreviousValue=true \
          ParameterKey=EC2ScaleUpCooldown,UsePreviousValue=true \
          ParameterKey=Environment,UsePreviousValue=true \
          ParameterKey=FailedMaxHealthCheckCount,UsePreviousValue=true \
