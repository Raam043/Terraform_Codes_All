aws cloudformation update-stack --stack-name $CF_STACK_NAME --use-previous-template --parameters \
          ParameterKey=ImageID,ParameterValue=$AMI_ID \
          ParameterKey=AppInstanceName,UsePreviousValue=true \
          ParameterKey=AppSubnet,UsePreviousValue=true \
          ParameterKey=CloudWatchHighEval,UsePreviousValue=true \
